view {
  box-sizing: border-box;
}
page {
  overflow: hidden;
}
.pageContainer {
  position: relative;
  width: 100%;
  height: 100%;
  overflow: hidden;
  background-color: #f5f5f5;
}
.pageContainer .meetingParams {
  width: 100%;
  background-color: transparent;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.pageContainer .meetingParams .btnContainer {
  width: 690rpx;
  margin-top: 34rpx;
  position: relative;
}
.pageContainer .meetingParams .btnContainer .joinBtn.show {
  display: block;
  z-index: 8;
}
.pageContainer .meetingParams .btnContainer .joinBtn.hide {
  display: none;
  z-index: 1;
}
.pageContainer .noticeModal ._modal-title {
  margin-bottom: 0;
}
.pageContainer .joinLoading {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 404rpx;
}
.pageContainer .joinLoading ._loading-icon {
  margin-bottom: 0;
  height: 80rpx;
  width: 80rpx;
}
.placeholderClass {
  letter-spacing: 0;
  text-align: left;
  color: #ccc;
  font-size: 32rpx;
  line-height: 108rpx;
  font-weight: 400;
}
.formItem {
  height: 108rpx;
  width: 100%;
  background-color: #fff;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 30rpx;
}
.formItem:first-of-type {
  margin-top: 24rpx;
  border-bottom: 1rpx solid #f5f5f5;
}
.formItem .formText {
  display: inline-block;
  width: 160rpx;
  color: #333;
  line-height: 108rpx;
}
.formItem .formInput {
  width: 70%;
  height: 70%;
  font-size: 32rpx;
  text-align: left;
  line-height: 108rpx;
}
.formForName {
  margin-bottom: 10rpx;
}
.switchContainer {
  width: 100%;
  height: 108rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 30rpx;
  background-color: #fff;
  box-sizing: border-box;
}
.switchContainer:not(:first-child) {
  border-top: 1rpx solid #f5f5f5;
}
.switchContainer .switchText {
  font-size: 32rpx;
  color: #333;
  font-weight: 400;
}
.switchContainer .switchBtn,
.switchContainer .switchBtn switch {
  width: 88rpx;
  height: 48rpx;
}
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #fff;
  width: 560rpx;
  height: 344rpx;
  overflow: hidden;
  padding: 48rpx 0 0;
  box-sizing: border-box;
  border-radius: 6rpx;
}
.title {
  font-size: 34rpx;
  color: #333;
  letter-spacing: 1rpx;
  text-align: center;
  line-height: 44rpx;
  font-weight: 700;
}
.btnWrapper {
  display: flex;
  justify-content: space-between;
  width: 100%;
  border-top: 1rpx solid rgba(25, 31, 37, 0.08);
  height: 88rpx;
}
.btnWrapper button {
  font-size: 30rpx;
  display: block;
  border: 0;
  border-radius: 0;
  width: 100%;
  line-height: 88rpx;
  text-align: center;
}
.btnWrapper button::active,
.btnWrapper button::focus,
.container .btnWrapper button::active,
.container .btnWrapper button::focus {
  border-radius: 0;
}
.btnWrapper button::after,
.container .btnWrapper button::after {
  border: 0;
}
.btnWrapper button.cancelBtn {
  background-color: #fff;
  color: #666;
  border-radius: 0;
}
.btnWrapper button.confirmBtn {
  background-color: #fff;
  font-weight: 400;
}
.errorContainer {
  height: 60rpx;
  line-height: 60rpx;
  width: calc(100% - 96rpx);
}
.errorText {
  text-align: left;
  font-size: 24rpx;
  color: #ff5151;
  letter-spacing: 0;
  font-weight: 400;
}
.input__container,
.input__error {
  width: calc(100% - 96rpx);
  height: 72rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 11rpx 0 16rpx;
  margin-top: 32rpx;
  box-sizing: border-box;
  flex-shrink: 0;
  position: relative;
  border: 2rpx solid #dfe2e5;
  border-radius: 4rpx;
}
.input__error::after {
  border: 1rpx solid #ff5151;
}
.input__title {
  font-size: 34rpx;
  color: #333;
  letter-spacing: 1rpx;
  text-align: center;
  line-height: 44rpx;
  font-weight: 500;
}
.input__input {
  flex: 1;
  color: #333;
  text-align: left;
  font-size: 28rpx;
  z-index: 999;
}
.input__suffixIcon {
  width: 40rpx;
  height: 40rpx;
  padding: 5rpx;
  z-index: 9;
}
.input__placeholderClass {
  font-size: 28rpx;
  color: #ccc;
  letter-spacing: 0;
  line-height: 44rpx;
  font-weight: 400;
  text-align: left;
}
.input__errorContainer {
  height: 60rpx;
  line-height: 60rpx;
  width: calc(100% - 96rpx);
}

.input__errorText {
  text-align: left;
  font-size: 24rpx;
  color: #ff5151;
  letter-spacing: 0;
  font-weight: 400;
}

